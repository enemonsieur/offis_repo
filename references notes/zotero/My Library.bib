@article{matthewsStressStatesPersonality2021,
  title = {Stress States, Personality and Cognitive Functioning: {{A}} Review of Research with the {{Dundee Stress State Questionnaire}}},
  shorttitle = {Stress States, Personality and Cognitive Functioning},
