  title = {Cognitive Workload Changes for Nurses Transitioning from a Legacy System with Paper Documentation to a Commercial Electronic Health Record},
  author = {Colligan, Lacey and Potts, Henry W.W. and Finn, Chelsea T. and Sinkin, Robert A.},
